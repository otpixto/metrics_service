{
    "name": "otpixto/metrics_service",
    "type": "library",
    "description": "Metrics service",
    "license": "MIT",
    "authors": [
      {
        "name": "Aleksei Sergeev",
        "email": "otpixto@gmail.com",
        "homepage": "https://github.com/otpixto"
      }
    ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^v11.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.1.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
